		<br />
		
		<div id="balle">Deposited AMOUNT : </div>
		<div id="abc"></div>
		
</div>

<script>
$(document).ready(function() {

$("#testone").change(function(){
	var test = $("#testone").val();
	$.ajax({
		url : "/new/ajax/balle",
		type : "GET",
		data : {"value": test},
		success:function(data){
			//alert(test);
			console.log(data);
			if (data.results) {
				
				$("#balle").append(data.results.converted);
			} if (data.interests) {
				var i = 1;
				var html="";
				$.each(data.interests,function(index,intrest){
					var aa = "interest"+i;
					//html+='<div id="int{}"'+i+'>'+intrest+'</div><br/>';
					//create some logic here as per your backend data structure
					var interestStatement = "Interest on ";
					if (i == 1) {
						interestStatement = "Interest on First 10000";
					} else if (i == 2) {
						interestStatement = "Interest On 1000 after First 10000";
					} else {
						interestStatement = "Interest on all amounts after 20000";
					}
					html += '<div id="'+index+'">'+interestStatement+" : " + intrest+'</div><br />';
					
					i++;
				});
				$("#abc").append(html);
			}
			
			//$("#balle").text("hello");
		}
	});
	
});
